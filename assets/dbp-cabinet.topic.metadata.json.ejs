{
  "name": {
    "de": "Elektronischer Studierendenakt",
    "en": "Electronic Student Records"
  },
  "short_name": {
    "de": "Studierendenakten Verwaltung",
    "en": "Manage student records"
  },
  "description": {
    "de": "Diese Applikation erm√∂glicht Ihnen die elektronische Verwaltung von Studierendenakten.",
    "en": " This application enables you to manage electronic student records."
  },
  "routing_name": "cabinet",
  "activities": [<%
      const allActivities = [
        { name: 'dbp-cabinet-search', path: 'dbp-cabinet-search.metadata.json' },
        { name: 'dbp-create-request', path: 'dbp-create-request.metadata.json' },
        { name: 'dbp-show-requests', path: 'dbp-show-requests.metadata.json' },
        { name: 'dbp-qualified-signature-pdf-upload', path: 'dbp-qualified-signature-pdf-upload.metadata.json' },
        { name: 'dbp-official-signature-pdf-upload', path: 'dbp-official-signature-pdf-upload.metadata.json' }
      ];
      const includedActivities = allActivities
        .filter(activity => activity.name in activities)
        .map(activity => `{"path": "${activity.path}"}`);
    %>
    <%- includedActivities.join(',\n    ') %>
  ],
  "attributes": []
}